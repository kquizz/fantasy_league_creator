<p style="color: green"><%= notice %></p>

<h2>Users</h2>

<div id="users">
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @users.each do |user| %>
    <%= render "user_row", user: user %>
  <% end %>
</table>
</div>

<%= link_to "New user", new_user_path, class: "new-link" %>
